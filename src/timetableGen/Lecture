package timetableGen;

import java.util.ArrayList;

public class Lecture extends Meeting{
	
	public static ArrayList<Meeting> lecturelist = new ArrayList<Meeting>();

	public Lecture(Day dayOfWeek, int startHour, int endHour) {
		super(dayOfWeek, startHour, endHour);
		// TODO Auto-generated constructor stub
		lecturelist.add(this);
	
	}

	public static ArrayList<Meeting> getLectureList(){
		return lecturelist;
	}
}
