package timetableGen;

import java.util.ArrayList;

public class MeetingSet {
	
	public static ArrayList<Meeting> courselist = new ArrayList<Meeting>(); //may be in singleton pattern
	
	
	public void generateCourseList(){
		for(int i=0;i<Tutorial.getTutList().size();i++){
			for(int j=0;i<Lecture.getLectureList().size();i++){
				courselist.add(Lecture.getLectureList().get(i));
				courselist.add(Tutorial.getTutList().get(i));
			}
		}
	}

}
