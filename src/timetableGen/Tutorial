package timetableGen;

import java.util.ArrayList;

public class Tutorial extends Meeting{
	
	public static ArrayList<Meeting> tutlist = new ArrayList<Meeting>();

	public Tutorial(Day dayOfWeek, int startHour, int endHour) {
		super(dayOfWeek, startHour, endHour);
		// TODO Auto-generated constructor stub
		tutlist.add(this);
	
	}

	public static ArrayList<Meeting> getTutList(){
		return tutlist;
	}
}
