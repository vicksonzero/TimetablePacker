package java;

import java.util.ArrayList;

public class Course {
	public ArrayList<String> courselist = new ArrayList<>(); //should be in singleton pattern
	public ArrayList<String> lecturelist = new ArrayList<>();
	public ArrayList<String> tutlist = new ArrayList<>();
	
	public void generateCourseList(){
		for(int i=0;i<tutlist.size();i++){
			for(int j=0;i<lecturelist.size();i++){
				courselist.add(lecturelist.get(i));
				courselist.add(tutlist.get(i));
			}
		}
	}

}
